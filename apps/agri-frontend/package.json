{
  "name": "agri-frontend",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@prisma/client": "^6.18.0",
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.548.0",
    "next": "~15.2.4",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.4.0"
  },
  "sourceRoot": "apps/agri-frontend/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "options": {
        "root": "apps/agri-frontend"
      },
      "outputs": [
        "{workspaceRoot}/dist/apps/agri-frontend"
      ]
    },
    "dev": {
      "executor": "@nx/next:dev-server",
      "options": {
        "root": "apps/agri-frontend",
        "port": 3000
      }
    },
    "serve": {
      "executor": "@nx/next:server",
      "options": {
        "root": "apps/agri-frontend"
      }
    }
  },
  "tags": []
}
